void p123(int n){
  int i=1;
  while(i<=n){
    int j=1;
    while(j<=n){
    cout<<"*";
    j=j+1;
  }
  i=i+1;
   cout<<endl;
  }
//   5
// *****
// *****
// *****
// *****
// *****
