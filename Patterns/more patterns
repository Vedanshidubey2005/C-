#include<iostream>
using namespace std;
void p12345(int n){
 for(int i=0;i<n;i++)  {
    for(int j=1;j<=n;j++){
        cout<<j;
    }
    cout<<endl;
 } 
//  5
// 12345
// 12345
// 12345
// 12345
// 12345
}
void p1234(int n){
//   for(int i=1;i<=n;i++)  {
//     for(int j=1;j<=n;j++){
//         cout<<n-j+1;
//     }
//     cout<<endl;
//  }   
int i=1;
while(i<=n){
  int j=1;
  while(j<=n){
    cout<< n-j+1;
    j=j+1;
  }
  i=i+1;
  cout<<endl;
}
