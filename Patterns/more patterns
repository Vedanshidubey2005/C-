}
void p333(int n){
   char ch='A';
  for(int i=1;i<=n;i++){
   
    for(int j=1;j<=i;j++){
      cout<< ch <<" ";
      ch=ch+1;
    }
    cout<<endl;
  }
//   1
// 5
// A 
// B C
// D E F
// G H I J
// K L M N O
}
int main()
{
 int t;
 cin>>t;
for(int i=0;i<t;i++){
int n;
cin>>n;
p333(n);
}

} 
