// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    //pairs
    pair<int,int>p={1,9};
    cout<<p.first<<" "<<p.second<<endl;  //1 9
    
    pair<int,string> p2(15,"Vedanshi");
    cout<<p2.first<<" "<<p2.second<<endl; //15 Vedanshi
    
    pair<int,double>p3;
   p3= make_pair(4,9.1);
   cout<<p3.first<<" "<<p3.second<<endl;  //4 9.1
    
    pair<int , pair<int,int>>p1= {23,{24,25}};
    cout<<p1.first<<" "<<p1.second.first<<" "<< p1.second.second<<endl;
    //23 24 25
 
    pair<int,int>arr[]={{1,2},{3,4},{5,6}};
    cout<<arr[0].second<<endl;  //2
    cout<<arr[1].second<<endl;  //4
    cout<<arr[2].first<<endl;   //5
    
    return 0;
}
